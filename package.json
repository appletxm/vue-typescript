{
  "name": "vue-typescript",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "clean": "rimraf dist/*",
    "eslint": "eslint --config .eslintrc.js src/js/**/*.*",
    "check-types": "tsc --watch --noEmit",
    "mock": "npm run check-types && node scripts/dev-server.js mock",
    "start": "npm run dev",
    "dev": "concurrently --kill-others \"npm run check-types\" \"node scripts/dev-server.js development\"",
    "debug": "npm run check-types && node --inspect scripts/dev-server.js development",
    "build": "npm run clean && node scripts/build.js production current-branch",
    "buildForProd": "npm run clean && node scripts/build.js production",
    "buildForTest": "npm run clean && node scripts/build.js production test",
    "buildForPre": "npm run clean && node scripts/build.js production pre",
    "release": "node scripts/release.js production"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "nprogress": "^0.2.0",
    "regenerator-runtime": "^0.13.5",
    "vue-router": "^3.1.3",
    "vuex": "^4.0.0-beta.2",
    "vue": "^3.0.1",
    "@vue/composition-api": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.10.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/compiler-sfc": "^3.0.1",
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "2.3.0",
    "chokidar": "^2.0.4",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^6.2.2",
    "event-source-polyfill": "0.0.12",
    "express": "4.16.3",
    "file-loader": "^6.0.0",
    "formidable": "^1.2.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.4",
    "ip": "^1.1.5",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "1.3.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "2.6.2",
    "terser-webpack-plugin": "^5.0.0",
    "typescript": "^3.9.5",
    "url-loader": "^2.1.0",
    "vue-loader": "16.0.0-beta.8",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.39.3",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "nodejs": {
    "version": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/vue-typescript"
  },
  "keywords": [
    "vue 3.0",
    "node",
    "typescript",
    "webpack"
  ],
  "author": "appletxm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appletxm/vue-typescript/issues"
  }
}
