{
  "name": "vue-typescript",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "clean": "rimraf dist/*",
    "eslint": "eslint --config .eslintrc.js src/js/**/*.js",
    "mock": "npm run clean && node scripts/dev-server.js mock",
    "start": "npm run clean && node scripts/dev-server.js development",
    "dev": "npm run clean && node scripts/dev-server.js development",
    "build": "npm run clean && npm run uglify && node scripts/build.js production current-branch",
    "buildForProd": "npm run clean && node scripts/build.js production",
    "buildForTest": "npm run clean && node scripts/build.js production test",
    "buildForPre": "npm run clean && node scripts/build.js production pre",
    "release": "node scripts/release.js production",
    "check-types": "tsc --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "vue-next": "0.0.1",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@vue/composition-api": "^0.5.0",
    "autoprefixer": "^9.6.1",
    "chalk": "2.3.0",
    "chokidar": "^2.0.4",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.8.0",
    "event-source-polyfill": "0.0.12",
    "express": "4.16.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "ora": "1.3.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "2.6.2",
    "scss-loader": "^0.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "webpack": "^4.39.3",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "nodejs": {
    "version": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appletxm/vue-typescript"
  },
  "keywords": [
    "vue-next",
    "node",
    "typescript",
    "webpack"
  ],
  "author": "appletxm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appletxm/vue-typescript/issues"
  }
}
